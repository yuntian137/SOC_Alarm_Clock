uint8_t first_Press;
void UpdateDisplay(uint8_t KeyValue)
{
    if (exKeyValue != 0XFF) // ÊùæÊâãÂâçÂè™Âá∫‰∏ÄÊ¨°ÈîÆ
    {
        gTkIsValid = 1;
        KeyValue = exKeyValue;
        if (first_Press == 0)
        {
            if (KeyValue == 17)
            {
                GPIO_TogglePins(GPIOC, GPIO_Pin_4);  // LED ÁÅØÁøªÔø?
                GPIO_TogglePins(GPIOC, GPIO_Pin_10); // LED ÁÅØÁøªÔø?
            }
            else if (KeyValue == 18)
            {
                GPIO_TogglePins(GPIOC, GPIO_Pin_5);  // LED ÁÅØÁøªÔø?
                GPIO_TogglePins(GPIOC, GPIO_Pin_11); // LED ÁÅØÁøªÔø?
            }
            first_Press = 1;
        }
    }
    else
    {
        gTkIsValid = 0;
        first_Press = 0;
    }
}

